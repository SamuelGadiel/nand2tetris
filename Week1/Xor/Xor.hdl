CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not (in=a, out=na);
    Not (in=b, out=nb);
    And (a=a, b=nb, out=x1);
    And (a=na, b=b, out=x2);
    Or (a=x1, b=x2, out=out);
}
